{
  "name": "recurun",
  "version": "0.1.13",
  "description": "Write recursive code that runs iteratively - avoid stack overflow with zero dependencies",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README_zh.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "node --test",
    "benchmark": "npx tsx benchmark/benchmark.ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "recursion",
    "trampoline",
    "tail-call",
    "generator",
    "stack-overflow",
    "typescript",
    "functional"
  ],
  "author": "RecuRun Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2234839/RecuRun.git"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "actions": {
    "node-version": "24"
  }
}
